  },
  "dependencies": {
    "@nextui-org/react": "^2.4.8",
    "classnames": "^2.5.1",
    "framer-motion": "^11.11.9",
    "plasmo": "0.89.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.20",
    "husky": "^8.0.0",
    "postcss": "^8.4.47",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.12",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
